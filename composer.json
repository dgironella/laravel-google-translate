{
    "name": "dgironella/laravel-google-translate",
    "description": "Laravel package for the Google Translate API ",
    "keywords": [
        "laravel",
        "package",
        "google",
        "translate",
        "google translate",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dani Gironella",
            "email": "david.gironella@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dgironella\\GoogleTranslate\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dgironella\\GoogleTranslate\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.2",
        "google/cloud-translate": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench" : "8.*",
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dgironella\\GoogleTranslate\\GoogleTranslateServiceProvider"
            ],
            "aliases":{
                "GoogleTranslate": "Dgironella\\GoogleTranslate\\GoogleTranslateFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
